<div class="mybar">
  <div class="logo_content">
    <div class="logo">
      <i class='bx bx-plus-medical'></i>
      <div class="logo_name">DocHub</div>
    </div>
    <i class='bx bx-menu' id="btnmenu"></i>
  </div>
  <ul class="nav_list">
    <li>
      <a href="/dashboard/all" id="allpatient">
        <i class='bx bx-folder-open'></i>
        <span class="links_name">My Patients</span>
      </a>
    </li>
    <li>
      <a href="/dashboard/upcoming">
        <i class='bx bx-calendar'></i>
        <span class="links_name">Upcoming Visits</span>
