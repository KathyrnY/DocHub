      </a>
    </li>
  </ul>
</div>

<script>
  let sideNav = document.querySelector(".mybar");
  sideNav.onclick = function (e) {
    console.log(e.target);
    const target = event.target;
    const isListItem = target.closest(".mybar li") !== null;
    // Check if the click was on a list item or outside
    if (!isListItem) {
      sideNav.classList.toggle("active");
    }
  }
</script>