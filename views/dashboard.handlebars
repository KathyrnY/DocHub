{{> sidenav}}
<div class="container navbar-padding">
  <h1>My Patients</h1>
  <div class="row">
    {{#each patientData as |PD|}}
    <div class="col-12 col-md-4 col-lg-3 mb-2">
      <div class="card">
        <div class="card-body">
          <h5 class="card-title">{{PD.first_name}} {{PD.last_name}}</h5>
          <p class="card-text">
            Age {{PD.age}} Years, {{PD.gender}}
            <br />
            Appointment Time: {{PD.appttime}}
          </p>
          </p>
          <a class="btn btn-primary" href="/dashboard/chart/{{PD.id}}">Get Info</a>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
</div>
<script src="/js/dashboard.js"></script>